{
  "name": "cypress-image-diff-html-report",
  "description": "A Github App for Cypress Image Diff",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier . --write --list-different",
    "lint": "eslint . --fix --max-warnings=0",
    "start": "netlify functions:serve --port 6868",
    "dev:ui": "vite dev --config src/ui/vite.config.js",
    "build:ui": "vite build --config src/ui/vite.config.js",
    "build:server": "tsc",
    "build": "run-s build:ui build:server",
    "type-check:ui": "vue-tsc --noEmit --project src/ui/tsconfig.json",
    "type-check:rest": "tsc --noEmit",
    "type-check": "run-s -c type-check:*"
  },
  "author": {
    "name": "Kien Ha Trung",
    "email": "kienht.dev@gmail.com"
  },
  "keywords": [
    "cypress",
    "visual",
    "testing",
    "regression",
    "comparison",
    "html report"
  ],
  "license": "MIT",
  "dependencies": {
    "@probot/adapter-aws-lambda-serverless": "^3.0.4",
    "adm-zip": "^0.5.12",
    "dayjs": "^1.11.11",
    "element-plus": "^2.6.0",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "octokit": "^3.2.1",
    "probot": "^13.2.1",
    "vue": "^3.3.4",
    "vue-router": "^4.3.2",
    "pinia": "^2.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@netlify/functions": "^2.7.0",
    "@types/adm-zip": "^0.5.5",
    "@types/fs-extra": "^11.0.2",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "smee-client": "^2.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.5",
    "vite-plugin-singlefile": "^0.13.5",
    "vue-tsc": "^1.8.5"
  },
  "type": "module"
}
